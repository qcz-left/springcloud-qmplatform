<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qcz.qmplatform.system.mapper.UserMapper">

    <select id="queryUserByUsername" resultType="com.qcz.qmplatform.system.vo.UserVo">
        select *
        from sys_user
        where username like #{username};
    </select>
</mapper>
